{
  "build": {
    "cpu": "cortex-m4",
    "f_cpu": "120000000L",
    "mcu": "mk64fn1m0vdc12"
  },
  "connectivity": [
    "bluetooth", 
    "can",
    "wifi"
  ],
  "debug": {
    "jlink_device": "MKW41Z512xxx4",
    "tools": {
      "cmsis-dap": {
        "server": {
          "package": "tool-pyocd",
          "executable": "$PYTHONEXE",
          "arguments": [
            "pyocd-gdbserver.py", 
             "-t", 
             "k64f"
          ]
        },
        "load_cmd": "preload"
      }
    }
  },
  "frameworks": [
    "mbed"
  ],
  "name": "Hexiwear",
  "upload": {
    "maximum_ram_size": 262144,
    "maximum_size": 1048576,
    "protocol": "mbed",
    "protocols": [
      "cmsis-dap",
      "jlink",
      "mbed"
    ]
  },
  "url": "https://developer.mbed.org/platforms/Hexiwear/",
  "vendor": "MikroElektronika"
}